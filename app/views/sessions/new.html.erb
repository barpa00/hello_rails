<h1>使用者登入</h1>
<% if notice%>
  <div class="w-full bg-yellow-100 px-8 py-4 rounded-md text-gray-500">
    <%= notice %>
  </div>
<% end %>
<%= form_with(model: @user, url: log_in_path , method: 'post', local: true) do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit t("button.submit") %>
<% end %>